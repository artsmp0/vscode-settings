    "Catppuccin",
    "Chakra",
    "clickhouse",
    "CLOUDINARY",
    "codeium",
    "codeup",
    "commitlint",
    "composables",
    "corepack",
    "cronstrue",
    "daisyui",
    "Datasource",
    "daterange",
    "datetime",
    "deepseek",
    "deepseeker",
    "depts",
    "echarts",
    "eqeqeq",
    "esbenp",
