{
  "breadcrumbs.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",

  "workbench.activityBar.location": "side",
  "workbench.editor.empty.hint": "text",
  "workbench.editor.labelFormat": "default",
  "workbench.editor.showTabs": "multiple",
  "workbench.iconTheme": "icons",
  "workbench.layoutControl.enabled": false,
  "workbench.list.smoothScrolling": true,
  "workbench.productIconTheme": "icons-carbon",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.statusBar.visible": true,
  "workbench.tree.indent": 16,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.colorTheme": "Vitesse Dark Soft",

  "window.nativeTabs": true,
  "window.nativeFullScreen": false,
  "window.commandCenter": false,
  "window.dialogStyle": "custom",
  "window.title": "${rootName}",
  "window.titleBarStyle": "custom",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.autoClosingBrackets": "languageDefined",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "FiraCode Nerd Font",
  "editor.fontLigatures": "'cv01', 'cv02', 'cv05', 'cv09', 'zero', 'ss04', 'ss03', 'cv31', 'cv29', 'cv27', 'ss10'",
  "editor.fontSize": 14.5,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.guides.indentation": false,
  "editor.hover.enabled": true,
  "editor.lightbulb.enabled": "off",
  "editor.lineHeight": 1.8,
  "editor.lineNumbers": "interval",
  "editor.matchBrackets": "never",
  "editor.minimap.enabled": true,
  "editor.renderLineHighlight": "gutter",
  "editor.renderWhitespace": "none",
  "editor.rulers": [120, 140],
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.wordSeparators": "`~!#%^\u0026*()=+[{]}\\|;:'\",.\u003c\u003e/?",

  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.sortOrder": "foldersNestsFiles",

  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontFamily": "FiraCode Nerd Font",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.stickyScroll.enabled": false,
  "terminal.integrated.tabs.enabled": true,

  // extension settings
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": false,
  "accessibility.verbosity.inlineChat": false,
  "extensions.ignoreRecommendations": true,
  "git.confirmSync": false,
  "github-enterprise.uri": "https://artsmp:ghu_2c7bb252603341e6a212c50b1a6f42a2@gc.aiu.im",
  "github.copilot.advanced": {
    "authProvider": "github-enterprise",
    "debug.chatOverrideProxyUrl": "https://gc.aiu.im/chat",
    "debug.overrideChatEngine": "gpt-4",
    "debug.overrideProxyUrl": "https://gc.aiu.im"
  },
  "prettier.printWidth": 140,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "eslint.quiet": true,
  "cSpell.userWords": [
    "antd",
    "antfu",
    "antv",
    "artsmp",
    "Attributify",
    "Cascader",
    "commitlint",
    "daisyui",
