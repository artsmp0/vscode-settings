{
  "breadcrumbs.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.productIconTheme": "icons-carbon",
  "workbench.iconTheme": "catppuccin-mocha",
  "workbench.activityBar.location": "top",
  "workbench.editor.empty.hint": "text",
  "workbench.editor.labelFormat": "default",
  "workbench.list.smoothScrolling": true,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.statusBar.visible": true,
  "workbench.tree.indent": 16,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.editor.customLabels.enabled": true,
  "workbench.editor.customLabels.patterns": {
    "**/views/**/index.vue": "${dirname} - View",
    "**/components/**/*.vue": "${filename} - Component",
    "**/utils/**/*.ts": "${filename} - Util",
    "**/hooks/**/*.ts": "${filename} - Hook",
    "**/composables/**/*.ts": "${filename} - Hook",
    "**/router/**/*": "${filename} - Route",
    "**/stores/**/*": "${filename} - Store",
    "**/styles/**/*": "${filename} - Style",
    "**/assets/**/*": "${filename} - Asset",
  },
  "workbench.colorCustomizations": {
    "[Vitesse Dark]": {
      "tab.activeBorder": "#4D9375",
      "list.inactiveFocusOutline": "#4D9375",
      "panel.border": "#4D9375",
      "sideBar.border": "#4D9375"
    }
  },

  "window.customTitleBarVisibility": "never",
  "window.nativeTabs": true,
  "window.nativeFullScreen": false,
  "window.commandCenter": false,
  "window.dialogStyle": "custom",
  "window.title": "${rootName}",
  "window.titleBarStyle": "custom",

  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.autoClosingBrackets": "languageDefined",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "MesloLGL Nerd Font Mono, Input",
  "editor.fontSize": 13.5,
  "editor.fontLigatures": true,
  "editor.codeLensFontSize": 10,
  "editor.suggestFontSize": 12,
  "editor.snippetSuggestions": "top",
  "editor.suggestLineHeight": 28,
  "editor.inlayHints.fontSize": 12,
  "errorLens.fontSize": "12px",
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.guides.indentation": false,
  "editor.hover.enabled": true,
  "editor.lightbulb.enabled": "off",
  "editor.lineHeight": 2,
  "editor.lineNumbers": "interval",
  "editor.matchBrackets": "never",
  "editor.minimap.enabled": true,
  "editor.renderLineHighlight": "gutter",
  "editor.renderWhitespace": "none",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.wordSeparators": "`~!#%^\u0026*()=+[{]}\\|;:'\",.\u003c\u003e/?",
  "editor.linkedEditing": true,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.tabSize": 2,
  "editor.tabCompletion": "on",

  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.sortOrder": "foldersNestsFiles",

  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontFamily": "FiraCode Nerd Font",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.stickyScroll.enabled": false,
  "terminal.integrated.tabs.enabled": true,

  // Search
  "search.smartCase": true,
  "search.useIgnoreFiles": false,
  "search.exclude": {
    "**/vendor/*": true,
    "**/public/*": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true,
    "**/pnpm-lock.json": true,
  },

  // extension settings
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
    ["cx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ],
  "diffEditor.hideUnchangedRegions.enabled": true,
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": false,
  "accessibility.verbosity.inlineChat": false,
  "extensions.ignoreRecommendations": true,
  "git.confirmSync": false,
  "gitlens.graph.layout": "editor",
  "github-enterprise.uri": "https://artsmp:ghu_2c7bb252603341e6a212c50b1a6f42a2@gc.aiu.im",
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.userWords": [
    "antd",
    "antfu",
    "antv",
    "artsmp",
    "Attributify",
    "bumpp",
    "Cascader",
    "Chakra",
    "clickhouse",
    "CLOUDINARY",
    "commitlint",
    "composables",
    "daisyui",
